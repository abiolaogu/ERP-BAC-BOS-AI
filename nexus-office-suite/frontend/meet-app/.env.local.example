# NEXUS Meet Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8095
NEXT_PUBLIC_WS_URL=ws://localhost:8095

# Application Configuration
NEXT_PUBLIC_APP_NAME=NEXUS Meet
NEXT_PUBLIC_APP_VERSION=1.0.0

# WebRTC Configuration
NEXT_PUBLIC_STUN_SERVERS=stun:stun.l.google.com:19302
NEXT_PUBLIC_MAX_PARTICIPANTS=50

# Feature Flags
NEXT_PUBLIC_ENABLE_RECORDING=true
NEXT_PUBLIC_ENABLE_SCREEN_SHARE=true
NEXT_PUBLIC_ENABLE_CHAT=true
